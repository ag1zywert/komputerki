isbn = {0092-6566},
issn = {00926566},
journal = {Journal of Research in Personality},
number = {1},
pages = {62--77},
pmid = {4483},
title = {{When Boy Meets Girl: Attractiveness and the Five-Factor Model in Opposite-Sex Interactions}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0092656600923043},
volume = {35},
